#include <iostream>
#include <vector>

using namespace std;
class Solution {

public:
  int mySqrt(int x) {

    int start = 0;
    int end = x;

    while (start <= end) {

      int mid = (start + end) / 2;
      int cond = x / mid;
      cout << cond << "ss" << endl;
      if (mid < cond)
        start = mid + 1;
      else if (mid > cond)
        end = mid - 1;
      else if (mid == cond)
        return mid;
    }
  }
};
int main() {

  Solution s;
  vector<int> nums = {4, 8, 16, 100};
  for (size_t i = 0; i < nums.size(); i++) {
    int x = s.mySqrt(nums[i]);
    cout << x << endl;
  }
  return 0;
}
